<!DOCTYPE html>
<html >
<head>
  <meta charset="UTF-8">
  <title>Custom <select></title>
  
  
  
      <link rel="stylesheet" href="css/style.css">
  

<script>
window.onload = function () {

    var xpointsstr = document.getElementById("xpoints").innerText;
    var ypointsstr = document.getElementById("ypoints").innerText;

    var xpoints = xpointsstr.split(" ");
    var ypoints = ypointsstr.split(" ");

    var dps = [];
    for (var i = 0; i <=xpoints.length - 1; i++) {
        //alert(obj.label +" "+ obj.y);
        
        var obj = {
            y: parseInt(ypoints[i]),
            label : xpoints[i]
        };
        dps.push(obj);
    }
    console.log(dps);

var chart = new CanvasJS.Chart("chartContainer", {
    animationEnabled: true,
    theme: "light2",
    title:{
        text: "Prediction"
    },
    axisY: {
        title: "Percentage"
    },
    data: [{        
        type: "column",  
        showInLegend: true, 
        legendMarkerColor: "grey",
        legendText: "",
        dataPoints: dps
    }]
});
chart.render();
}
</script>
<style>
